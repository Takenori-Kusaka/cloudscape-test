{
  "name": "management-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-vite": "tsc && vite build",
    "preview": "vite preview",
    "webpack": "NODE_ENV=production webpack",
    "clean": "node ./scripts/clean",
    "generate-html-files": "node ./scripts/generate-html-files",
    "jest": "jest ./test/**/*.test.ts",
    "build": "npm-run-all -s generate-html-files webpack",
    "pretest": "npm-run-all -s clean lint typecheck build",
    "typecheck": "tsc",
    "start": "NODE_ENV=development webpack-dev-server",
    "start-no-live-reload": "node ./scripts/dev-server.js",
    "start:https": "webpack-dev-server --https --port=4443 --host=$(hostname)",
    "test-no-build": "run-p -r start-no-live-reload jest",
    "test": "npm run test-no-build",
    "prepare": "husky install",
    "lint": "eslint --fix --ignore-path .gitignore --ext ts,tsx,js,jsx,json ."
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.628",
    "@cloudscape-design/global-styles": "^1.0.27",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "lint-staged": {
    "*.html": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "package-lock.json": [
      "./scripts/clean-package-lock.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@cloudscape-design/board-components": "^3.0.60",
    "@cloudscape-design/browser-test-tools": "^3.0.53",
    "@cloudscape-design/code-view": "^3.0.16",
    "@cloudscape-design/collection-hooks": "^1.0.45",
    "@cloudscape-design/component-toolkit": "^1.0.0-beta.48",
    "@cloudscape-design/components": "^3.0.628",
    "@cloudscape-design/design-tokens": "^3.0.36",
    "@cloudscape-design/global-styles": "^1.0.27",
    "@cloudscape-design/test-utils-core": "^1.0.31",
    "@cloudscape-design/theming-runtime": "^1.0.49",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ace-builds": "^1.33.1",
    "axe-core": "^4.9.1",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "cpy": "^11.0.1",
    "css-loader": "^7.1.1",
    "date-fns": "^3.6.0",
    "del": "^7.1.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.1",
    "sass-loader": "^14.2.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "vite": "^5.2.11",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
